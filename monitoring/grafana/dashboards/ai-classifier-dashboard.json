{
  "dashboard": {
    "id": null,
    "title": "AI Image Classification Service Dashboard",
    "tags": ["ai", "classification", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "ai_classifier_system_status",
            "legendFormat": "System Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "ai_classifier_cpu_percent",
            "legendFormat": "CPU Usage %"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ]
      },
      {
        "id": 3,
        "title": "Memory Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "ai_classifier_memory_percent",
            "legendFormat": "Memory Usage %"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "max": 100,
            "unit": "percent"
          }
        ]
      },
      {
        "id": 4,
        "title": "Classification Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "ai_classifier_avg_processing_time",
            "legendFormat": "Avg Processing Time"
          },
          {
            "expr": "ai_classifier_classifications_per_minute",
            "legendFormat": "Classifications/min"
          }
        ],
        "yAxes": [
          {
            "min": 0,
            "unit": "s"
          }
        ]
      },
      {
        "id": 5,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "ai_classifier_db_active_connections",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "ai_classifier_db_total_connections",
            "legendFormat": "Total Connections"
          }
        ]
      },
      {
        "id": 6,
        "title": "Cache Performance",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "ai_classifier_cache_hit_rate",
            "legendFormat": "Cache Hit Rate %"
          },
          {
            "expr": "ai_classifier_cache_memory_usage",
            "legendFormat": "Cache Memory Usage"
          }
        ]
      },
      {
        "id": 7,
        "title": "Model Usage Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "ai_classifier_model_usage_count",
            "legendFormat": "{{model_name}}"
          }
        ]
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "ai_classifier_error_rate",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Active Users",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 28},
        "targets": [
          {
            "expr": "ai_classifier_active_users",
            "legendFormat": "Active Users"
          }
        ]
      },
      {
        "id": 10,
        "title": "Total Classifications Today",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 28},
        "targets": [
          {
            "expr": "ai_classifier_classifications_today",
            "legendFormat": "Classifications Today"
          }
        ]
      }
    ]
  }
}